{
	"version": "1.0",
	"defaultProvider": "cdnjs",
	"libraries": [
		{
			"library": "bootstrap@5.1.3",
			"destination": "wwwroot/lib/bootstrap/"
		},
		{
			"library": "bootstrap-icons@1.8.3",
			"destination": "wwwroot/lib/bootstrap-icons/"
		},
		{
			"library": "jquery@3.6.0",
			"destination": "wwwroot/lib/jquery/"
		},
		{
			"library": "jquery-validate@1.19.4",
			"destination": "wwwroot/lib/jquery-validate/"
		},
		{
			"library": "jquery-validation-unobtrusive@3.2.12",
			"destination": "wwwroot/lib/jquery-validation-unobtrusive/"
		},
		{
			"library": "tinymce@6.0.3",
			"destination": "wwwroot/lib/tinymce/"
		},
		{
			"library": "flag-icon-css@6.6.0",
			"destination": "wwwroot/lib/flag-icon-css/"
		},
		{
			"provider": "unpkg",
			"library": "persian-date@1.1.0",
			"destination": "wwwroot/lib/persian-date/",
			"files": [
				"dist/persian-date.js",
				"dist/persian-date.min.js",
				"doc/document.md",
				"doc/leapyear.md"
			]
		},
		{
      "provider": "unpkg",
			"library": "persian-datepicker@1.2.0",
			"destination": "wwwroot/lib/persian-datepicker/",
			"files": [
				"dist/index.html",
				"dist/initial-value.html",
				"dist/mobile.html",
				"dist/range.html",
				"dist/single.html",
				"dist/css/theme/persian-datepicker-blue.min.css",
				"dist/css/theme/persian-datepicker-blue.min.css.map",
				"dist/css/theme/persian-datepicker-cheerup.min.css",
				"dist/css/theme/persian-datepicker-cheerup.min.css.map",
				"dist/css/theme/persian-datepicker-dark.min.css",
				"dist/css/theme/persian-datepicker-dark.min.css.map",
				"dist/css/theme/persian-datepicker-redblack.min.css",
				"dist/css/theme/persian-datepicker-redblack.min.css.map",
				"dist/css/persian-datepicker.css",
				"dist/css/persian-datepicker.min.css",
				"dist/css/persian-datepicker.min.css.map",
				"dist/doc/API.md",
				"dist/doc/OPTIONS.md",
				"dist/js/persian-datepicker.js",
				"dist/js/persian-datepicker.min.js"
			]
		}
	]
}
